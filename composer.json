{
    "name": "laraxot/module_notify_fila",
    "description": "notify module dedicated to managing notifications",
    "keywords": [
        "laraxot",
        "laravel",
        "filament",
        "module_notify",
        "Laravel",
        "Notify module",
        "Notify",
        "Emails",
        "Smss",
        "Whatsapp"
    ],
    "homepage": "https://github.com/laraxot/module_notify_fila.git",
    "license": "MIT",
    "authors": [{
        "name": "Marco Sottana",
        "email": "marco.sottana@gmail.com",
        "role": "Developer"
    }],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Notify\\Providers\\NotifyServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Notify\\": ""
        }
    },
    "require": {
        "filament/filament": "*",
        "illuminate/contracts": "*",
        "illuminate/support": "*",
		"kreait/laravel-firebase": "^5.3",
        "laraxot/module_media_fila3": "dev-master",
        "laraxot/module_xot_fila3": "dev-master",
        "phpdocumentor/type-resolver": "*",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "*",
        "driftingly/rector-laravel": "^0.26.2",
        "enlightn/enlightn": "^2.5",
        "fakerphp/faker": "*",
        "friendsofphp/php-cs-fixer": "*",
        "laravel/pint": "^1.13",
        "nesbot/carbon": "*",
        "nette/php-generator": "*",
        "larastan/larastan": "^2.6",
        "nunomaduro/phpinsights": "^2.9",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpunit/phpunit": "*",
        "psalm/plugin-laravel": "^2.8",
        "rector/phpstan-rules": "^0.7.1",
        "rector/rector": "^0.18.6",
        "spatie/laravel-typescript-transformer": "*",
        "spatie/pest-plugin-snapshots": "*",
        "spatie/phpunit-snapshot-assertions": "*",
        "spatie/test-time": "*",
        "symplify/phpstan-rules": "^12.4",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "tightenco/duster": "^2.2",
        "vimeo/psalm": "^5.15"
    },
    "repositories": [ {
        "type": "path",
        "url": "../Xot"
    },
    {
        "type": "path",
        "url": "../Tenant"
    },
    {
        "type": "path",
        "url": "../UI"
    },
    {
        "type": "path",
        "url": "../Media"
    }],
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
